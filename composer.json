    "type": "typo3-cms-extension",
    "require": {
        "typo3/cms-core": "^11.5"
    },
    "license": "GPL-2.0",
    "extra": {
        "typo3/cms": {
            "extension-key": "arc_mjml"
        }
    },
    "autoload": {
        "psr-4": {
            "Archriss\\ArcMjml\\": "Classes/"
        }
    },
    "authors": [
        {
            "name": "Christophe Monard",
            "email": "cmonard@archriss.com"
        }
    ]
